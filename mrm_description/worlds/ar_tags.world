<?xml version="1.0" ?>

<sdf version="1.5">
	<world name="default">
		<!-- A global light source -->
		<include>
			<uri>model://sun</uri>
		</include>

    <!-- Martian Gravity-->
    <gravity>0 0 -3.711</gravity>

		<!-- A ground plane -->
		<include>
			<uri>model://ground_plane</uri>
		</include>

        <!-- Our custom model -->
		<include>
			<uri>model://ar_tags</uri>
			<name>curiosity_path</name>
		</include>
        
	</world>
</sdf>